<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>
	<managed-bean>
		<description>This for the send bid screen</description>
		<managed-bean-name>bidForm</managed-bean-name>
		<managed-bean-class>
			com.merrill.examples.client.ui.form.bid.BidForm
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<description>
			This for handling actions on the bid screen
		</description>
		<managed-bean-name>bidController</managed-bean-name>
		<managed-bean-class>
			com.merrill.examples.client.controller.bid.BidController
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>form</property-name>
			<property-class>
				com.merrill.examples.client.ui.form.bid.BidForm
			</property-class>
			<value>#{bidForm}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bidForm1</managed-bean-name>
		<managed-bean-class>
			com.merrill.examples.client.ui.form.bid.BidForm1
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bidController1</managed-bean-name>
		<managed-bean-class>
			com.merrill.examples.client.controller.bid.BidController1
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>form</property-name>
			<property-class>
				com.merrill.examples.client.ui.form.bid.BidForm1
			</property-class>
			<value>#{bidForm1}</value>
		</managed-property>
	</managed-bean>




</faces-config>